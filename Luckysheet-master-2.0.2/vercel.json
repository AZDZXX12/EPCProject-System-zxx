{
  "version": 2,
  "name": "luckysheet-selection-system",
  "builds": [
    {
      "src": "dist-refactored/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist-refactored/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/dist-refactored/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/dist-refactored/index.html"
    },
    {
      "source": "/fan-selector.html",
      "destination": "/dist-refactored/fan-selector.html"
    },
    {
      "source": "/cable-selector/(.*)",
      "destination": "/dist-refactored/cable-selector/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        }
      ]
    }
  ]
}

